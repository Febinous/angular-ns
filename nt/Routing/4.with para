-- appModules

const appRoutes:Routes=[{path:'users/:id/:name',component:UsersComponent}]

constructor(private route:ActivatedRoute){}

ngOnInit(){
    this.user={id:this.route.snapshot.params['id'],
    name:this.route.snapshot.params['name']}}
}




